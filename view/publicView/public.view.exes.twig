{% extends 'template.html.twig' %}

{% block title %} Public | View Executables {% endblock %}
{% block content %}

{% for code in getCode %}
    <div class="text-left border border-gray-700 pt-2 px-6 pb-6 mt-28">
        <h3 class="text-3xl text-center underline">{{ code.getSnipExesTitle | capitalize }}</h3>
        <h4 class="text-2xl text-center">{{ code.getSnipExesDesc }}</h4>
        <h4 class="text-2xl text-center">Executable Files</h4>
    </div>

    {% endfor %}
{% endblock %}
{% block javascript %}
{{ parent() }}
<script src="scripts/copyCodeScript.js"></script>
<script src="../node_modules/flowbite/dist/flowbite.js"></script>
{% endblock %}
